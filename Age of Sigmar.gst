        <profile id="d525-c9a4-8f8a-584f" name="高崔克·冈尼森" hidden="false" typeId="1960-ca8e-67ce-2014" typeName="单位">
          <characteristics>
            <characteristic name="移动力" typeId="8655-6213-2824-1752">4寸</characteristic>
