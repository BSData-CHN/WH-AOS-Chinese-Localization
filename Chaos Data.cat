    <selectionEntry id="812a-a8b3-7158-4adb" name="Slaves to Darkness" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="01c2-4a03-1c0e-189f" type="max"/>
