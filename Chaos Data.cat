        <profile id="c6f5-96b9-85ff-64f6" name="Predatory" hidden="false" typeId="c924-5a68-471a-2fd5" typeName="Unit Abilities">
          <characteristics>
            <characteristic name="Ability Details" typeId="d4dc-8e81-bc0e-b8f0">A Daemonic Simulacrum is a predatory endless spell. It can move up to 9&quot; and can fly.</characteristic>
          </characteristics>
        </profile>
