        <constraint field="selections" scope="roster" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="e13d-d95f-7ddf-d6b3" type="min"/>
      </constraints>
      <selectionEntries>
        <selectionEntry id="9dd2-3e59-e0f3-441f" name="1. Best of the Best" hidden="false" collective="false" import="true" type="upgrade">
          <profiles>
            <profile id="f756-3fa5-4bee-2352" name="Best of the Best" hidden="false" typeId="f71f-b0a4-730e-ced3" typeName="Command Abilities">
